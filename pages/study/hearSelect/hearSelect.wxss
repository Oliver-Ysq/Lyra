}

.optionWrapper {
  display: flex;
  justify-content: space-evenly;
  align-items: flex-start;
}

.optionIndex {
  width: 12vmin;
  height: 12vmin;
  border-radius: 50%;
  color: #5B63A8;
  font-size: 4vmin;
  border: 0.27vmin solid #BDB9F6;
  display: flex;
  justify-content: center;
  align-items: center;
}

.optionItem {
  width: 78vmax;
}

.picked {
  color: #3654FD;
  border: 0.5vmin solid #3654FD;
  font-weight: bold;
}

.upTitle {
  margin: 6vmin 8vmin 6vmin 8vmin;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.type {
  font-size: 5.33vmin;
  color: #333333;
}

.score {
  font-size: 3.73vmin;
  color: #333333;
}

.scoreNum {
  font-size: 6vmin;
  color: #3654FD;
}

.questions {
  margin-left: 8vmin;
  margin-right: 8vmin;
  height: 45vmin;
  overflow: auto;
}

.btnWrapper {
  width: 100%;
  height: 20vmin;
  display: flex;
  justify-content: center;
  align-items: flex-start;
}

.checkBtn {
  width: 35.5vmax;
  height: 11.73vmin;
  border-radius: 22.4vmin;
  background: #FF7498;
  color: white;
  font-size: 4vmin;
  display: flex;
  justify-content: center;
  align-items: center;
}

.circle {
  display: inline-flex !important;
  width: 12.8vmin;
  height: 12.8vmin;
  border-radius: 50%;
  justify-content: center;
  align-items: center;
  font-size: 3.73vmin;
  margin-right: 5vmin;
  margin-bottom: 3vmin;
}

.wrong {
  color: #D35925;
  border: 0.53vmin solid #D35925;
}

.true {
  color: #5EA253;
  border: 0.27vmin solid #5EA253;
}

.nav {
  height: 11.73vmin;
  width: 100%;
  position: relative;
}

.navback {
  position: absolute;
  left: 2vmin;
  top: 1vmin;
}

/*list*/
.commodity_attr_box {
  width: 100%;
  overflow: scroll;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  background: #fff;
  height: 99vmin;
  text-align: center;
  padding-bottom: 4vmin;
}

.listHead {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.listTitle {
  font-size: 3.73vmin;
  height: 11.7vmin;
  line-height: 11.7vmin;
}

#back {
  border-radius: 50%;
  color: #4c69ff;
  font-size: 8.5vmin;
  position: absolute;
  left: 3.2vmin;
  top: 0rpx;
}

.cell {
  display: flex;
  flex-direction: row;
  height: 12vmin;
  width: 100vmax;
}

.cell-index {
  flex: 1;
  text-align: right;
  height: 12vmin;
  line-height: 12vmin;
  font-size: 4.3vmin;
  color: black;
}

.cell-state {
  flex: 5;
  text-align: right;
  height: 12vmin;
  line-height: 12vmin;
  padding-right: 0.8vmin;
  font-size: 3.2vmin;
}

.tmp {
  flex: 1;
}

.boundary {
  width: 90vmax;
  margin-left: auto;
  margin-right: auto;
  height: 0.27vmin;
  background: #F0EDFF;
}

.option-img {
  width: 100%;
}

.main-title {
  font-size: 4.6vmin;
  margin: 20px;
  margin-bottom: 14px;
}

.iconBlue {
  font-size: 6vmin;
  font-weight: bold;
  color: #3654FD;
}

@keyframes rotate {
  0% {
    transform: rotate(0);
  }

  50% {
    transform: rotate(180deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

.rotate {
  transition: 0.5s;
  /* transform-origin: 30px 30px;   */
  animation: rotate 10s linear infinite;
  /*开始动画后无限循环，用来控制rotate*/
}

.standard {
  padding-left: 0.9vmin;
  padding-right: 1vmin;
  display: flex;
  justify-content: center;
  align-items: center;
}

.skip {
  /* margin-left: 1vmin; */
  padding-left: 2vmin;
  padding-right: 2vmin;
}

.beatWrapper {
  margin-left: 2vmin;
