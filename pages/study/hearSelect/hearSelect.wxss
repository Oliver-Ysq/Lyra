.layout {
  box-sizing: border-box;
  width: 100vmax;
  padding-left: 5vmax;
  padding-right: 5vmax;
  margin-left: auto;
  margin-right: auto;
  height: 100vmin;
  background: rgb(251, 250, 255);
  overflow: scroll;
  position: relative;
  margin-top: 11.73vmin;
}

.content {}

.floating {
  background-color: #FFFFFF95;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 10vmin;
  padding-left: 1vmin;
  padding-right: 2vmin;
  border: 1px solid;
  border-color: #3654FD20;
  border-radius: 4vmin;
  position: fixed;
  z-index: 9;
  font-size: 14px;
  box-shadow: 0px 2px 3px 3px #9E9AC520;
  color: #3654FD
}

.audioWrapper {
  width: 20vmax;
  /* height: 6vmin; */
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  margin-right: 2vmin;
}


.topWrapper {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 5;
}

.top {
  height: 11.73vmin;
  width: 100vmax;
  background: rgb(254, 254, 255);
  position: relative;
}

.back {
  position: absolute;
  width: 5.4vmax;
  height: 100%;
  left: 0;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.list {
  position: absolute;
  width: 5.4vmax;
  height: 100%;
  right: 0;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.top-main {
  box-sizing: border-box;
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: center;
  padding-left: 9.6vmin;
}

.last {
  box-sizing: border-box;
  width: 16.8vmax;
  height: 9.6vmin;
  border-radius: 15.47vmin;
  border: 0.27vmin solid #3654FD;
  color: #3654FD;
  font-size: 3.73vmin;

  display: flex;
  justify-content: center;
  align-items: center;
}
.last-hover {
  box-sizing: border-box;
  width: 16.8vmax;
  height: 9.6vmin;
  border-radius: 15.47vmin;
  border: 0.27vmin solid #3654FD;
  color: #3654FD;
  font-size: 3.73vmin;
  background-color:#3654FD20;

  display: flex;
  justify-content: center;
  align-items: center;
}

.next {
  box-sizing: border-box;
  width: 16.8vmax;
  height: 9.6vmin;
  border-radius: 15.47vmin;
  font-size: 3.73vmin;
  border: 0.27vmin solid #3654FD;
  color: #3654FD;

  display: flex;
  justify-content: center;
  align-items: center;
}
.next-hover{

  box-sizing: border-box;
  width: 16.8vmax;
  height: 9.6vmin;
  border-radius: 15.47vmin;
  font-size: 3.73vmin;
  border: 0.27vmin solid #3654FD;
  color:#3654FD;
  background-color:#3654FD20;

  display: flex;
  justify-content: center;
  align-items: center;

}
.top-title {
  color: #4E4A80;
  font-size: 3.73vmin;
}

.myicon {
  font-size: 6vmin;
  font-weight: bold;
  color: #3654FD;
}

.title {
  width: 100%;
  height: 13.3vmin;
  display: flex;
  align-items: center;
  margin-bottom: 8px;
}

.index {
  font-size: 16px;
  display: inline-block;
  width: 8.8vmin;
  height: 8.8vmin;
  border-radius: 50%;
  color: 1px solid #5B63A8;
  background: #EAE8FC;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 4vmin;
  margin-right: 2vmin;


}

.title-text {
  display: inline-block;
  font-size: 3.73vmin;
  color: #333333;
  display: flex;
  justify-content: center;
  align-items: center;
}

.detail {
  font-size: 2.93vmin;
  color: #333333;
  text-align: right;
  margin-right: 4vmin;
}

.puli {
  width: 100%;
  min-height: 46.67vmin;
  margin-left: auto;
  margin-right: auto;
}



.bgblue {
  background: #3654FD;
}

.blue {
  background: rgb(93, 118, 253);
  color: white;
}

.bggrey {
  background: rgba(54, 84, 253, .4);
}

.progress {
  width: 88%;
}

.stop {
  margin-left: 1vmin;
