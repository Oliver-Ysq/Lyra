/* 遮罩层样式 */

.isRuleShow {
  display: block;
}

.isRuleHide {
  display: none;
}

.ruleZhezhao {
  height: 100%;
  width: 100%;
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  top: 0;
  left: 0;
  z-index: 999;
}

.ruleZhezhaoContent {
  padding: 40rpx 0;
  width: 60%;
  background: #fff;
  margin: 40% auto;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  position: relative;
}

.title {
  font-size: 40rpx;
}

#imgContent {
  display: flex;
  flex-direction: row;
}

/* 遮罩层结束 */

.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 10;
  background: #fff;
  /* padding-top: 20rpx; */
  height: 544rpx;
}

.commodity_attr_box {
  text-align: center;
}

#back {
  margin-right: 24rpx;
  border-radius: 50%;
  color: #4c69ff;
  font-size: 64rpx;
}

#dots {
  background: #f0edff;
  position: absolute;
  left: 0;
  top: 0;
  height: 100rpx;
  width: 66.7%;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f0edff;
  border-top: 1px solid #f0edff;
  border-radius: 20rpx;
}

#dots::before {
  content: '';
  position: absolute;
  top: 100rpx;
  left: 12.5%;
  transform: translateX(-18rpx);
  border-left: 15rpx dashed transparent;
  border-right: 15rpx dashed transparent;
  border-top: 36rpx dashed rgb(240, 237, 255);
}

#dots view {
  width: 25%;
  position: relative;
}

#sheng {
  background: #f0edff;
  position: absolute;
  left: 0;
  top: 0;
  height: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f0edff;
  border-top: 1px solid #f0edff;
  border-radius: 20rpx;
}

#sheng::before {
  content: '';
  position: absolute;
  top: 100rpx;
  left: 30%;
  transform: translateX(-18rpx);
  border-left: 15rpx dashed transparent;
  border-right: 15rpx dashed transparent;
  border-top: 36rpx dashed rgb(240, 237, 255);
}

#sheng view {
  width: 20%;
  position: relative;
}

#xiu {
  background: #f0edff;
  position: absolute;
  left: 0;
  top: 0;
  height: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f0edff;
  border-top: 1px solid #f0edff;
  border-radius: 20rpx;
}

#xiu::before {
  content: '';
  position: absolute;
  top: 100rpx;
  left: 41.7%;
  transform: translateX(-18rpx);
  border-left: 15rpx dashed transparent;
  border-right: 15rpx dashed transparent;
  border-top: 36rpx dashed rgb(240, 237, 255);
}

#xiu view {
  width: 16.7%;
  position: relative;
}

#notes {
  height: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f0edff;
  border-top: 1px solid #f0edff;
}

#notesBox {
  height: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  border-bottom: 1px solid #f0edff;
}

#notes view {
  width: 16.67%;
  border-right: 1px solid #f0edff;
  position: relative;
}

#notesBox>view {
  width: 16.67%;
  border-right: 1px solid #f0edff;
  position: relative;
}

#upload {
  background: #ff7498;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 32rpx;
  color: white;
}

.scroll_box {
  position: relative;
  height: 244rpx;
  overflow: hidden;
  white-space: nowrap;
}

.white {
  box-sizing: border-box;
  height: 244rpx;
  display: inline-block;
  position: relative;
  
}

.black {
  height: 152rpx;
  position: absolute;
  background: rgb(58, 53, 107);
}

.down-arrow {
  margin-top: 12rpx;
  margin-right: 4rpx;
  float: right;
  width: 30rpx;
  height: 30rpx;
}

.down-arrow:after {
  content: '';
  display: block;
  width: 16rpx;
  height: 16rpx;
  border-right: 2rpx solid rgb(230, 230, 230);
  border-top: 2rpx solid rgb(230, 230, 230);
  -webkit-transform: rotate(-45deg); /*箭头方向可以自由切换角度*/
  transform: rotate(-45deg);
}

.arrow0 {
  margin-top: 45rpx;
  margin-right: 4rpx;
  float: right;
  width: 30rpx;
  height: 30rpx;
}

.arrow0:after {
  content: '';
  display: block;
  width: 15rpx;
  height: 15rpx;
  border-right: 3rpx solid rgb(164, 160, 200);
  border-top: 3rpx solid rgb(164, 160, 200);
  transform: rotate(-45deg);
}

.arrow1 {
  margin-top: 16rpx;
  margin-right: 4rpx;
  float: right;
  width: 30rpx;
  height: 30rpx;
}

.arrow1:after {
  content: '';
  display: block;
  width: 15rpx;
  height: 15rpx;
  border-left: 3rpx solid rgb(164, 160, 200);
  border-bottom: 3rpx solid rgb(164, 160, 200);
  transform: rotate(-45deg);
}

.dialog-noBorder {
  width: 550rpx;
  height: 90rpx;
  background: white;
  bottom: 560rpx;
  left: 20rpx;
  border-radius: 20rpx;
  position: fixed;
  border: 4rpx dashed #9e9ac5;
  text-align: center;
}

.dialog-noBorder::before {
  content: '';
  position: absolute;
  bottom: 30rpx;
  left: 546rpx;
  border-left: 36rpx dashed white;
  border-top: 15rpx dashed transparent;
  border-bottom: 15rpx dashed transparent;
}

.progress {
  box-sizing: border-box;
  position: absolute !important;
  bottom: 33rpx;
  right: 10%;
  width: 380rpx !important;
}

.progress-item {
}

.reset {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  left: 5%;
  bottom: 15rpx;
}

.score {
  position: absolute;
  right: 0;
  top: 0;
  height: 74rpx;
  width: 20%;
  text-align: center;
  line-height: 74rpx;
  font-size: 34rpx;
  color: #ff7498;
}

.uploadTop {
  padding-top: 14rpx;
  width: 80%;
  height: 60rpx;
  position: relative;
  color: #ff7498;
}

#uploadButtons {
  height: 106rpx;
  width: 100%;
  display: flex;
  justify-content: space-evenly;
  flex-direction: row;
  align-items: center;
}

.uploadButton {
  width: 140rpx;
  height: 54rpx;
  border: 2rpx solid #ff7498;
  border-radius: 60rpx;
  background: white;
  font-size: 26rpx;
  text-align: center;
  line-height: 54rpx;
  color: #ff7498;
}

.comment {
  float: left;
  width: 450rpx;
  overflow: scroll;
  margin-left: 40rpx;
  margin-top: 24rpx;
  height: 100rpx;
  font-size: 28rpx;
}
.commodity {
  width: 100%;
