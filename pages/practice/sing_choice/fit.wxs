//*********************取整函数 */
var filters = {

  toFix: function (value) {
    value=value+''
    if (value.length == 2) {
      return value+''
    } else if (value.length == 1) {
      return '0' + value
    }



  },
  toPer:function(num,total){
    return num/total
  }

}
module.exports = {

  toFix: filters.toFix,
  toPer: filters.toPer,

}