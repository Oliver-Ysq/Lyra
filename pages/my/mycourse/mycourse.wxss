.layout {
  width: 100vw;
  height: 100vh;
  overflow: scroll;
  background: #FBFAFF;
}

.top {
  width: 100%;
  height: 208rpx;
  display: flex;
  flex-direction: row;
}

.topitem {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
  align-items: center;
  background: #FBFAFF;
}

.topnumber {
  margin-top: 16rpx;
  font-size: 68rpx;
  color: #7A7A7A;
  font-weight: bold;
  display: flex;
  justify-content: center;
  align-items: center;
}

.toptext {
  font-size: 28rpx;
  color: #7A7A7A;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 12rpx;
}

.blk {
  height: 100rpx;
  background: white;
}

.pointTitle {
  font-size: 36rpx;
  height: 120rpx;
  line-height: 120rpx;
  margin-left: 36rpx;
  font-weight: bold;
}

.card{
  width: 100vw;
  height: 190rpx;
  padding-left: 36rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: white;
  position: relative;
}

.cardimg{
  width: 206rpx;
  height: 144rpx;
  border-radius: 10rpx;
}

.cardTitle{
  font-size: 32rpx;
}

.cardtext{
  font-size: 24rpx;
  color: #7A7A7A;
  margin-left: 10rpx;
}
.marginLeft{
  margin-left: 12rpx;
}

.cardInfo{
  padding-left: 32rpx;
}

.arrow{
  position: absolute;
  right: 32rpx;
  font-size: 40rpx;
  top: calc(85rpx - 20rpx);
  color: #D0CDEF;
  font-weight: bold;
}

.boundary{
  height: 16rpx;
  width: 100%;
}

.passtext{
  display: inline-block;
  line-height: 32rpx;
}

.titleWrapper{
  display: flex;
  flex-direction: row;
  align-items: center;
}